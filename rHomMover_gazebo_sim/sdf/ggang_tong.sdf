<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>1 1 1 1</diffuse>
      <specular>0.5 0.5 0.5 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>
    <gravity>0 0 -9.8</gravity>

    <model name="ground_plane">
        <static>true</static>
        <link name="link">
            <collision name="collision">
            <geometry>
                <plane>
                <normal>0 0 1</normal>
                </plane>
            </geometry>
            </collision>
            <visual name="visual">
            <geometry>
                <plane>
                <normal>0 0 1</normal>
                <size>100 100</size>
                </plane>
            </geometry>
            <material>
                <ambient>0.8 0.8 0.8 1</ambient>
                <diffuse>0.8 0.8 0.8 1</diffuse>
                <specular>0.8 0.8 0.8 1</specular>
            </material>
            </visual>
        </link>
    </model>


    <model name="joint_controller_demo">
      <static>false</static>
      <pose>0 0 0.5 0 0 0</pose>

      <link name="base_link">
        <pose>0.0 0.0 0.0 0 0 0</pose>
        <inertial>
          <inertia>
            <ixx>2.501</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>2.501</iyy>
            <iyz>0</iyz>
            <izz>5</izz>
          </inertia>
          <mass>1.0</mass>
        </inertial>
        <visual name="base_visual">
          <pose>0.0 0.0 0.0 0 0 0</pose>
          <geometry>
            <box>
              <size>1.0 0.8 0.2</size>
            </box>
          </geometry>
          
        </visual>
        <collision name="base_collision">
          <pose>0.0 0.0 0.0 0 0 0</pose>
          <geometry>
            <box>
              <size>1.0 0.8 0.2</size>
            </box>
          </geometry>
        </collision>
      </link>

      <link name="left_arm">
        <pose>0.5 0.6 0.0 1.5707963 0 0</pose>
        <inertial>
          <inertia>
            <ixx>2.501</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>2.501</iyy>
            <iyz>0</iyz>
            <izz>5</izz>
          </inertia>
          <mass>0.5</mass>
        </inertial>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.1 0.2</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
          <transparency>0.5</transparency>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.1 0.1 0.2</size>
            </box>
          </geometry>
        </collision>
      </link>

      <link name="left_wheel">
        <pose>0.5 0.8 0.0 1.5707963 0 0</pose>
        <inertial>
          <inertia>
            <ixx>0.032</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.032</iyy>
            <iyz>0</iyz>
            <izz>0.00012</izz>
          </inertia>
          <mass>0.5</mass>
        </inertial>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>0.1</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
          <transparency>0.5</transparency>
        </visual>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>0.1</length>
            </cylinder>
          </geometry>
        </collision>
      </link>

      <link name="right_arm">
        <pose>0.5 -0.6 0.0 1.5707963 0 0</pose>
        <inertial>
          <inertia>
            <ixx>2.501</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>2.501</iyy>
            <iyz>0</iyz>
            <izz>5</izz>
          </inertia>
          <mass>0.5</mass>
        </inertial>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.1 0.2</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
          <transparency>0.5</transparency>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.1 0.1 0.2</size>
            </box>
          </geometry>
        </collision>
      </link>

      <link name="right_wheel">
        <pose>0.5 -0.8 0.0 1.5707963 0 0</pose>
        <inertial>
          <inertia>
            <ixx>0.032</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.032</iyy>
            <iyz>0</iyz>
            <izz>0.00012</izz>
          </inertia>
          <mass>0.5</mass>
        </inertial>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>0.1</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
          <transparency>0.5</transparency>
        </visual>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>0.1</length>
            </cylinder>
          </geometry>
        </collision>
      </link>
 
      <!-- <joint name="world_fixed" type="fixed">
        <parent>world</parent>
        <child>base_link</child>
      </joint> -->
 
      
      <joint name="left_arm_joint" type="revolute">
        <pose>0 0 0.1 0 0 0</pose>
        <parent>base_link</parent>
        <child>left_arm</child>
        <axis>
          <xyz>0 1 0</xyz>
        </axis>
        <!-- 여기는 arm이므로 리미트 걸어줘야 함. 확인할 것. 그리고 플러그인도 포지션 모드로 해줘야 함.  -->
      </joint>
      <joint name="left_wheel_joint" type="revolute">
        <pose>0 0 -0.1 0 0 0</pose>
        <parent>left_arm</parent>
        <child>left_wheel</child>
        <axis>
          <xyz>0 0 -1</xyz>
        </axis>
      </joint>

      <joint name="right_arm_joint" type="revolute">
        <pose>0 0 -0.1 0 0 0</pose>
        <parent>base_link</parent>
        <child>right_arm</child>
        <axis>
          <xyz>0 1 0</xyz>
        </axis>
        <!-- 여기는 arm이므로 리미트 걸어줘야 함. 확인할 것. 그리고 플러그인도 포지션 모드로 해줘야 함.  -->
      </joint>
      <joint name="right_wheel_joint" type="revolute">
        <pose>0 0 0 0 0 0</pose>
        <parent>right_arm</parent>
        <child>right_wheel</child>
        <axis>
          <xyz>0 0 -1</xyz>
        </axis>
      </joint>

      <plugin
        filename="gz-sim-joint-controller-system"
        name="gz::sim::systems::JointController">
        <joint_name>left_wheel_joint</joint_name>
        <initial_velocity>0.0</initial_velocity>
        <topic>left_wheel_vel</topic>
      </plugin>

      <plugin
        filename="gz-sim-joint-controller-system"
        name="gz::sim::systems::JointController">
        <joint_name>right_wheel_joint</joint_name>
        <initial_velocity>0.0</initial_velocity>
        <topic>right_wheel_vel</topic>
      </plugin>

      <!-- 아래는 팔 관련 플러그인 -->
      <plugin
        filename="gz-sim-joint-position-controller-system"
        name="gz::sim::systems::JointPositionController">
        <joint_name>right_arm_joint</joint_name>
        <topic>right_arm_angle</topic>
        <p_gain>10</p_gain>
        <i_gain>0.1</i_gain>
        <d_gain>0.1</d_gain>
        <i_max>1</i_max>
        <i_min>-1</i_min>
        <cmd_max>1000</cmd_max>
        <cmd_min>-1000</cmd_min>
      </plugin>

      <plugin
        filename="gz-sim-joint-position-controller-system"
        name="gz::sim::systems::JointPositionController">
        <joint_name>left_arm_joint</joint_name>
        <topic>left_arm_angle</topic>
        <p_gain>10</p_gain>
        <i_gain>0.1</i_gain>
        <d_gain>0.1</d_gain>
        <i_max>1</i_max>
        <i_min>-1</i_min>
        <cmd_max>1000</cmd_max>
        <cmd_min>-1000</cmd_min>
      </plugin>

      <!-- 여기에 joint state 발행하는거 넣어야 함.  -->


    </model>
  </world>
</sdf>
